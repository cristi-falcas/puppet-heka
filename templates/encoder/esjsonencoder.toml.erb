[<%= @name -%>]
type = "ESJsonEncoder"
index = "<%= index -%>"
type_name = "<%= type_name -%>"
<% if @fields then -%>fields = "<%= fields -%>"
<% end -%>
timestamp = "<%= timestamp -%>"
es_index_from_timestamp = <%= es_index_from_timestamp %>
<% if @id then -%>id = "<%= id -%>"
<% end -%>
<% if @raw_bytes_fields then -%>raw_bytes_fields = "<%= raw_bytes_fields -%>"
<% end -%>

<% if @field_mappings then -%>
[<%= @name -%>.field_mappings]
<% @field_mappings.each do |key, value| if value -%>
<%= key -%> = "<%= value -%>"
<% end end -%>
<% end -%>
