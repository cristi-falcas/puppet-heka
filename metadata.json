{
  "name": "cristifalcas-heka",
  "version": "1.4.0",
  "author": "Falcas Cristian",
  "summary": "Installs and configures heka",
  "license": "Apache-2.0",
  "source": "git@github.com:cristifalcas/puppet-heka.git",
  "project_page": "https://github.com/cristifalcas/puppet-heka",
  "issues_url": "https://github.com/cristifalcas/puppet-heka/issues",
  "tags": ["heka", "logging"],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
        "name": "yo61/logrotate",
        "version_requirement": ">= 1.3.0 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
